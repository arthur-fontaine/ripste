{
	"$schema": "https://turborepo.com/schema.json",
	"tasks": {
		"build": {
			"cache": true,
			"dependsOn": ["^build"]
		},
		"dev": {
			"persistent": true,
			"cache": false
		},
		"start": {
			"persistent": true,
			"cache": false
		},
		"test": {
			"cache": true,
			"dependsOn": ["^build"]
		},
		"test:watch": {
			"cache": false,
			"persistent": true
		}
	}
}
